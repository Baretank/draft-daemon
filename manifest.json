{
  "manifest_version": 3,
  "name": "Draft Caddy",
  "version": "1.0",
  "description": "Helps you track exposures and week 17",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "runAt": "document_idle",
      "matches": [
        "https://underdogfantasy.com/draft/*"
      ]
    }
  ]
}
